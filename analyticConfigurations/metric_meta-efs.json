{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "aws.efs.burstcreditbalance",
        "properties": {
          "statistic": "avg"
        }
      },
      {
        "match": "aws.efs.clientconnections",
        "properties": {
          "statistic": "sum"
        }
      },
      {
        "match": "aws.efs.datawriteiobytes",
        "properties": {
          "statistic": "sum"
        }
      },
      {
        "match": "aws.efs.metadataiobytes",
        "properties": {
          "statistic": "sum"
        }
      },
      {
        "match": "aws.efs.percentiolimit",
        "properties": {
          "tags": {
            "unit": "percent",
            "utilization": true
          }
        }
      },
      {
        "match": "aws.efs.permittedthroughput",
        "properties": {
          "statistic": "avg"
        }
      },
      {
        "match": "aws.efs.totaliobytes",
        "properties": {
          "statistic": "sum"
        }
      }
    ],
    "name": "Elastic File System",
    "scope": {
      "elementType": "EFS"
    },
    "type": "METRIC_META"
  }
}